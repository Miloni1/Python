vowel_eater = ""
user_word  = str(input("Enter word of your choice = "))
user_word = user_word.upper()
print("Word entered by you is:",user_word)
for letter in user_word:
    if letter == "A":
        word = letter
        continue
    elif letter =="E" :
        continue
    elif letter =="I":
        continue
    elif letter =="O":
        continue
    elif letter =="U":
        continue
    else:
        vowel_eater += letter  
    
#for i in vowel_eater:
    #print(i)
print(vowel_eater)    
